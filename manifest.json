{
    "manifest_version": 3,
    "name": "Juejin helper",
    "description": "Provide useful feature for juejin website",
    "version": "1.0",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.juejin.cn/*",
                "https://juejin.cn/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}